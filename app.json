{
    "name": "String",
    "description": "A bot for Generating String Session of Telegram Account mostly for Userbots.",
    "repository": "https://github.com/fjgaming212/String_Session_Pyrogram",
    "keywords": [
        "python",
        "telegram"
    ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "env": {
        "APP_NAME": {
            "description": "Heroku App Name",
            "value": "",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Bot token, untuk buat string.",
            "value": "",
            "required": true
        },
        "API_KEY": {
            "description": "Akun Heroku API Key dapatkan di https://dashboard.heroku.com/account ",
            "value": "",
            "required": true
        },
        "API_ID": {
            "description": "Dapatkan di https://my.telegram.org",
            "value": "",
            "required": true
        },
        "API_HASH": {
            "description": "Dapatkan di https://my.telegram.org",
            "value": "",
            "required": true
        }
    }
}
